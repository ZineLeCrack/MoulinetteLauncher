s1 = "42", s2 = "", n = 1  -->  returned value = OK
s1 = "", s2 = "42", n = 1  -->  returned value = OK
s1 = "42", s2 = "42", n = 5  -->  returned value = OK
s1 = "", s2 = "", n = 3  -->  returned value = OK
